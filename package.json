{
  "name": "gcp-mcp",
  "version": "1.0.2",
  "description": "Model Context Protocol (MCP) server for Google Cloud Platform - Talk to your GCP resources",
  "main": "index.ts",
  "bin": {
    "gcp-mcp": "./bin.js"
  },
  "scripts": {
    "start": "tsx index.ts",
    "start:server": "tsx src/server.ts",
    "start:client": "tsx src/claude-desktop-client.ts",
    "build": "tsc",
    "dev": "tsx watch index.ts",
    "dev:server": "tsx watch src/server.ts",
    "test": "tsx bin.js"
  },
  "keywords": [
    "gcp",
    "mcp",
    "claude",
    "ai",
    "llm"
  ],
  "author": "Oluwapelumi Oluwaseyi",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/bigquery": "^7.9.2",
    "@google-cloud/billing": "^4.6.0",
    "@google-cloud/billing-budgets": "^4.3.0",
    "@google-cloud/compute": "^4.12.0",
    "@google-cloud/container": "^5.19.0",
    "@google-cloud/functions": "^3.6.1",
    "@google-cloud/logging": "^11.2.0",
    "@google-cloud/resource-manager": "^5.3.1",
    "@google-cloud/run": "^1.5.1",
    "@google-cloud/secret-manager": "^5.6.0",
    "@google-cloud/sql": "^0.19.1",
    "@google-cloud/storage": "^7.15.0",
    "@modelcontextprotocol/sdk": "^1.6.1",
    "express": "^4.18.2",
    "google-auth-library": "^9.0.0",
    "googleapis": "^146.0.0",
    "node-ssh": "^13.2.0",
    "ts-morph": "^24.0.0",
    "tsx": "^4.19.2",
    "ws": "^8.16.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.10.2",
    "@types/ws": "^8.5.10",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eniayomi/gcp-mcp.git"
  },
  "bugs": {
